using System;
using System.Collections.Generic;
using System.IO;

class Solution
{
    static void Main(String[] args)
    {
        int i = 4;
        double d = 4.0;
        string s = "HackerRank ";


        // Declare second integer, double, and String variables.
        int ii;

        double dd;

        String ssString;
        // Read and save an integer, double, and String to your variables.

        ii = Convert.ToInt32(Console.ReadLine());
        dd = Convert.ToDouble(Console.ReadLine());
        ssString = Console.ReadLine();

        // Print the sum of both integer variables on a new line.
        Console.WriteLine(ii + i);
        // Print the sum of the double variables on a new line. "{0:F1}"
        Console.WriteLine("{0:F1}",d + dd);
        // Concatenate and print the String variables on a new line
        Console.WriteLine(s + ssString);
        // The 's' variable above should be printed first.
    }
}
